#include <stdio.h>
#include <stdlib.h>

//stoppa in i node?
void rev(char x[])
  {
    int i = 9;
    while(i >= 0)
      {
        putchar(x[i]);
        i--;
      }
  }

void recout(char x[], int i)
  {
    putchar(x[i]);
    if(i > 0)
      {
        i--;
        recout(x, i);
      }
  }

int main()
{
  char vec[10];
  for(int i = 0; i < 10; i++)
    {
      vec[i] = getchar();
    }

  //rev(vec);
  recout(vec, 9);
  printf("\n");
  rev(vec);
}
